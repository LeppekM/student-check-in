<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="totalTabPage" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="InventoryController.ControllerTotalTab">
  <children>
    <Label layoutX="20.0" layoutY="14.0" text="Total Inventory">
      <font>
        <Font size="24.0" fx:id="x1" />
      </font>
    </Label>
    <AnchorPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <TableView id="tableInv" fx:id="tableView" prefHeight="475.0" prefWidth="820.0" AnchorPane.bottomAnchor="59.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="66.0">
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
          <columns>
            <TableColumn editable="false" prefWidth="155.0" text="Part Name" fx:id="partName" />
            <TableColumn editable="false" prefWidth="52.0" text="Serial No." fx:id="serialNumber" />
            <TableColumn editable="false" prefWidth="78.0" text="Location" fx:id="location" />
            <TableColumn editable="false" prefWidth="68.0" text="Barcode" fx:id="barcode" />
            <TableColumn editable="false" prefWidth="129.0" text="Fault" fx:id="fault" />
            <TableColumn editable="false" prefWidth="89.0" text="Part ID" fx:id="partID" />
          </columns>
        </TableView>
      </children>
    </AnchorPane>
    <Button fx:id="add" layoutX="560.0" layoutY="554.0" mnemonicParsing="false" onAction="#addPart" prefHeight="33.0" prefWidth="106.0" text="Add" />
    <Button fx:id="remove" layoutX="680.0" layoutY="554.0" mnemonicParsing="false" onAction="#removePart" prefHeight="33.0" prefWidth="106.0" text="Remove" />
    <Button fx:id="refresh" layoutX="713.0" layoutY="20.0" mnemonicParsing="false" onAction="#populateTable" text="Refresh" />
  </children>
</AnchorPane>
