                <TableColumn fx:id="sNameCol" prefWidth="75.0" text="Student Name" />
                <TableColumn fx:id="partNameCol" prefWidth="75.0" text="Part Name" />
            <TableColumn fx:id="quantityCol" prefWidth="75.0" text="Quantity" />
            <TableColumn fx:id="checkOutAtCol" prefWidth="75.0" text="Checked Out Date" />
