            fx:controller="Popups.CheckoutPopUpController">

    <Pane prefHeight="283.0" prefWidth="410.0">
        <Label layoutX="42.0" layoutY="13.0" text="Student Name:">
            <font>
                <Font size="18.0" fx:id="x1"/>
            </font>
        </Label>
        <JFXTextField fx:id="name" alignment="CENTER" editable="false" focusColor="FIREBRICK" layoutX="175.0"
                      layoutY="14.0" prefHeight="23.94140625" prefWidth="153.5078125"/>
        <Label font="$x1" layoutX="73.0" layoutY="54.0" text="Part Name:"/>
        <JFXTextField fx:id="part" alignment="CENTER" editable="false" focusColor="FIREBRICK" layoutX="175.0"
                      layoutY="54.0" prefHeight="23.94140625" prefWidth="153.5078125"/>
