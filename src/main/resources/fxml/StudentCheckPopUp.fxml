        <JFXTextField fx:id="barcode" alignment="CENTER" editable="false" focusColor="FIREBRICK" layoutX="175.0"
                      layoutY="94.0" prefHeight="23.94140625" prefWidth="153.5078125"/>
        <Label font="$x1" layoutX="32.0" layoutY="134.0" text="Checked Out On:"/>
        <JFXTextField fx:id="coDate" alignment="CENTER" editable="false" focusColor="FIREBRICK" layoutX="175.0"
                      layoutY="134.0" prefHeight="23.94140625" prefWidth="153.5078125"/>
        <Label font="$x1" layoutX="93.0" layoutY="174.0" text="Due Date:"/>
        <JFXTextField fx:id="dueDate" alignment="CENTER" editable="false" focusColor="FIREBRICK" layoutX="175.0"
                      layoutY="174.0" prefHeight="23.94140625" prefWidth="153.5078125"/>
        <Label fx:id="cID" layoutX="7.0" layoutY="261.0" style="-fx-font-size: 14;" text="Checkout ID:"/>
    </Pane>
</AnchorPane>
