<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXCheckBox?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="main" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="700.0" prefWidth="789.0" stylesheets="@../css/PageStyle.css"
            xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Popups.EditWorker">
   <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0"
         prefWidth="789.0">
      <Button layoutX="732.0" layoutY="16.0" mnemonicParsing="false" onAction="#save"
              style="-fx-background-color: #ffffff; -fx-background-radius: 15pt; -fx-border-radius: 15pt; -fx-border-color: #043993; -fx-text-fill: #000000;">
         <graphic>
            <ImageView fitHeight="35.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
               <Image url="@../images/save.png"/>
            </ImageView>
         </graphic>
      </Button>
      <VBox fx:id="vbox" layoutX="40.0" layoutY="16.0" prefHeight="402.0" prefWidth="668.0"
            style="-fx-text-fill: red; -fx-effect: innershadow( three-pass-box , rgba(0,0,0,0.7) , 6, 0.0 , 0 , 2 );">
         <JFXTextField fx:id="email" alignment="CENTER" focusColor="FIREBRICK" prefHeight="100.0"
                       promptText="MSOE Email"
                       style="-fx-effect: innershadow( three-pass-box , rgba(0,0,0,0.7) , 6, 0.0 , 0 , 2 );">
            <font>
               <Font name="Arial Black" size="32.0"/>
            </font>
         </JFXTextField>
         <JFXTextField fx:id="workerName" alignment="CENTER" focusColor="FIREBRICK" prefHeight="100.0"
                       promptText="Name (First and Last)"
                       style="-fx-effect: innershadow( three-pass-box , rgba(0,0,0,0.7) , 6, 0.0 , 0 , 2 );">
            <VBox.margin>
               <Insets/>
            </VBox.margin>
            <font>
               <Font name="Arial Black" size="32.0"/>
            </font>
         </JFXTextField>
         <JFXTextField fx:id="eRFIDw" alignment="CENTER" focusColor="FIREBRICK" layoutX="10.0" layoutY="110.0"
                       prefHeight="100.0" promptText="RFID"
                       style="-fx-effect: innershadow( three-pass-box , rgba(0,0,0,0.7) , 6, 0.0 , 0 , 2 );">
            <font>
               <Font name="Arial Black" size="32.0"/>
            </font>
         </JFXTextField>
         <Pane prefHeight="100.0" prefWidth="200.0">
            <JFXPasswordField fx:id="pass" alignment="CENTER" prefHeight="100.0" prefWidth="670.0"
                              promptText="Password"
                              style="-fx-effect: innershadow( three-pass-box , rgba(0,0,0,0.7) , 6, 0.0 , 0 , 2 );">
               <font>
                  <Font name="Arial Black" size="32.0"/>
               </font>
            </JFXPasswordField>
            <JFXTextField fx:id="unmasked" alignment="CENTER" focusColor="FIREBRICK" prefHeight="100.0"
                          prefWidth="670.0" promptText="Password"
                          style="-fx-effect: innershadow( three-pass-box , rgba(0,0,0,0.7) , 6, 0.0 , 0 , 2 );">
               <font>
                  <Font name="Arial Black" size="32.0"/>
               </font>
            </JFXTextField>
         </Pane>
      </VBox>
      <VBox layoutX="61.0" layoutY="419.0" prefHeight="281.0" prefWidth="668.0">
         <HBox prefHeight="142.0" prefWidth="200.0">
            <JFXCheckBox fx:id="admin" alignment="CENTER" checkedColor="FIREBRICK" mnemonicParsing="false"
                         onAction="#unblock" prefHeight="130.0" text="Assign Privileges?">
               <HBox.margin>
                  <Insets left="150.0"/>
               </HBox.margin>
               <font>
                  <Font name="Arial Black" size="32.0"/>
               </font>
            </JFXCheckBox>
         </HBox>
         <HBox prefHeight="142.0" prefWidth="200.0">
            <VBox prefHeight="200.0" prefWidth="100.0">
               <HBox.margin>
                  <Insets left="25.0" right="75.0"/>
               </HBox.margin>
               <JFXCheckBox fx:id="editParts" checkedColor="FIREBRICK" disable="true"
                            mnemonicParsing="false" prefHeight="142.0" text="Editing Parts">
                  <font>
                     <Font name="Arial Black" size="24.0"/>
                  </font>
               </JFXCheckBox>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <JFXCheckBox fx:id="workers" checkedColor="FIREBRICK" disable="true" mnemonicParsing="false"
                            prefHeight="142.0" text="Manage Workers">
                  <font>
                     <Font name="Arial Black" size="24.0"/>
                  </font>
               </JFXCheckBox>
               <JFXCheckBox fx:id="removeParts" checkedColor="FIREBRICK" disable="true"
                            mnemonicParsing="false" prefHeight="142.0" text="Removing Parts">
                  <font>
                     <Font name="Arial Black" size="24.0"/>
                  </font>
               </JFXCheckBox>
            </VBox>
         </HBox>
      </VBox>
      <JFXCheckBox fx:id="showPass" checkedColor="FIREBRICK" layoutX="680.0" layoutY="320.0" mnemonicParsing="false"
                   prefHeight="100.0" prefWidth="107.0" text="Show Password" wrapText="true">
         <font>
            <Font name="Arial Black" size="14.0"/>
         </font>
      </JFXCheckBox>
   </Pane>
</AnchorPane>
