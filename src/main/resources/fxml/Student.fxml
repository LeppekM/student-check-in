<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="main" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
      prefHeight="800" prefWidth="1150" stylesheets="@../css/PageStyle.css" xmlns="http://javafx.com/javafx/10.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="CheckItemsController.StudentPage">
   <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
      <Button mnemonicParsing="false" onAction="#goBack" prefHeight="50.0" prefWidth="100.0"
              stylesheets="@../css/CheckButton.css" HBox.hgrow="ALWAYS">
         <graphic>
            <ImageView fitHeight="35.0" fitWidth="39.0" nodeOrientation="INHERIT" pickOnBounds="true">
               <viewport>
                  <Rectangle2D/>
               </viewport>
               <Image url="@../images/back.png"/>
            </ImageView>
         </graphic>
         <HBox.margin>
            <Insets bottom="210.0" left="10.0" right="10.0" top="10.0"/>
         </HBox.margin>
      </Button>
      <VBox fx:id="vbox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            prefHeight="279.0" prefWidth="664.0" HBox.hgrow="ALWAYS">
      </VBox>
      <Button maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#goHome" prefHeight="50.0"
              prefWidth="100.0" stylesheets="@../css/CheckButton.css" HBox.hgrow="ALWAYS">
         <graphic>
            <ImageView fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <Image url="@../images/home.png"/>
            </ImageView>
         </graphic>
         <HBox.margin>
            <Insets bottom="210.0" left="10.0" right="10.0" top="10.0"/>
         </HBox.margin>
      </Button>
   </HBox>
   <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="342.0"
         prefWidth="789.0" VBox.vgrow="ALWAYS">
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            prefHeight="346.0" prefWidth="265.0" HBox.hgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="10.0" right="5.0"/>
         </VBox.margin>
         <HBox.margin>
            <Insets right="10.0"/>
         </HBox.margin>
         <Label stylesheets="@../css/HeaderStyle.css" text="Checked Out" VBox.vgrow="ALWAYS">
            <padding>
               <Insets bottom="10.0"/>
            </padding>
         </Label>
         <JFXTreeTableView fx:id="coTable" layoutX="20.0" onMouseClicked="#coPopUp"
                           stylesheets="@../css/TableStyle.css" VBox.vgrow="ALWAYS">
            <VBox.margin>
               <Insets bottom="10.0"/>
            </VBox.margin>
         </JFXTreeTableView>
      </VBox>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            prefHeight="346.0" prefWidth="265.0" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="10.0"/>
         </HBox.margin>
         <Label stylesheets="@../css/HeaderStyle.css" text="Overdue" VBox.vgrow="ALWAYS">
            <padding>
               <Insets bottom="10.0"/>
            </padding>
         </Label>
         <JFXTreeTableView fx:id="oTable" onMouseClicked="#oPopUp" stylesheets="@../css/TableStyle.css"
                           VBox.vgrow="ALWAYS">
            <VBox.margin>
               <Insets bottom="10.0"/>
            </VBox.margin>
         </JFXTreeTableView>
      </VBox>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            prefHeight="346.0" prefWidth="265.0" HBox.hgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="10.0"/>
         </VBox.margin>
      </VBox>
   </HBox>
</VBox>
