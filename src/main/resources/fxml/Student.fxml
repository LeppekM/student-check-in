            <Button mnemonicParsing="false" onAction="#goBack" style="-fx-background-color: #ffffff; -fx-background-radius: 15pt; -fx-border-radius: 15pt; -fx-border-color: #043993; -fx-text-fill: #000000;" HBox.hgrow="ALWAYS">
               <graphic>
                  <ImageView fitHeight="35.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/back.png" />
                     </image>
                     <viewport>
                        <Rectangle2D />
                     </viewport>
                  </ImageView>
               </graphic>
               <HBox.margin>
                  <Insets bottom="210.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <VBox fx:id="vbox" alignment="CENTER" maxWidth="664.0" prefHeight="279.0" prefWidth="664.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin></VBox>
            <Button mnemonicParsing="false" onAction="#goHome" style="-fx-background-color: #ffffff; -fx-background-radius: 15pt; -fx-border-radius: 15pt; -fx-border-color: #043993; -fx-text-fill: #000000;" HBox.hgrow="ALWAYS">
               <graphic>
                  <ImageView fitHeight="35.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/home.png" />
                     </image>
                  </ImageView>
               </graphic>
               <HBox.margin>
                  <Insets bottom="210.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="342.0" prefWidth="789.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox alignment="CENTER" prefHeight="346.0" prefWidth="265.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label stylesheets="@../css/HeaderStyle.css" text="Checked Out" VBox.vgrow="ALWAYS">
                     <padding>
                        <Insets bottom="10.0" />
                     </padding>
                  </Label>
                  <JFXTreeTableView fx:id="coTable" layoutX="20.0" onMouseClicked="#coPopUp" prefHeight="272.0" prefWidth="220.0" stylesheets="@../css/TableStyle.css" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="10.0" right="5.0" />
                     </VBox.margin>
                  </JFXTreeTableView>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" right="5.0" />
               </VBox.margin>
            </VBox>
            <VBox alignment="CENTER" prefHeight="346.0" prefWidth="265.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label stylesheets="@../css/HeaderStyle.css" text="Overdue" VBox.vgrow="ALWAYS">
                     <padding>
                        <Insets bottom="10.0" />
                     </padding>
                  </Label>
                  <JFXTreeTableView fx:id="oTable" onMouseClicked="#oPopUp" prefHeight="272.0" prefWidth="220.0" stylesheets="@../css/TableStyle.css" VBox.vgrow="ALWAYS">
