<VBox fx:id="main" alignment="CENTER" stylesheets="@../css/PageStyle.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CheckItemsController.StudentPage">
   <children>
      <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
