<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="overduePage" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="InventoryController.ControllerOverdueTab">
   <children>
       <Label layoutX="15.0" layoutY="11.0" text="Overdue Items">
           <stylesheets>
               <URL value="@HeaderStyle.css" />
           </stylesheets>
       </Label>
      <Pane prefHeight="600.0" prefWidth="800.0">
         <children>
            <TableView fx:id="overdueTable" layoutY="65.0" prefHeight="476.0" prefWidth="800.0">
              <columns>
                <TableColumn fx:id="studentID" editable="false" minWidth="159.0" prefWidth="155.0" text="Student ID" />
                <TableColumn fx:id="partID" editable="false" minWidth="159.0" prefWidth="180.0" text="Part" />
                  <TableColumn fx:id="serial" editable="false" minWidth="159.0" prefWidth="155.0" text="Serial Number" />
                  <TableColumn fx:id="date" editable="false" minWidth="159.0" prefWidth="155.0" text="Original Due Date" />
                  <TableColumn fx:id="price" editable="false" minWidth="159.0" prefWidth="155.0" text="Fee" />
              </columns>
                <stylesheets>
                    <URL value="@TableStyle.css" />
                </stylesheets>
            </TableView>
         </children>
      </Pane>
   </children>
</AnchorPane>
