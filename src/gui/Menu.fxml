<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="mainMenuScene" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <StackPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <VBox alignment="CENTER">
               <children>
              <Label style="-fx-font-weight: bold" text="MSOE Technical Support Center" underline="true">
                <font>
                  <Font size="36.0" />
                </font>
                <VBox.margin>
                  <Insets bottom="25.0" />
                </VBox.margin>
              </Label>
                  <HBox alignment="CENTER">
                     <children>
                        <VBox>
                           <children>
                          <Button fx:id="checkInOutButtonMenuPage" mnemonicParsing="false" prefHeight="75.0" prefWidth="150.0" text="Check In/Out">
                                 <VBox.margin>
                                    <Insets bottom="50.0" />
                                 </VBox.margin></Button>
                          <Button fx:id="inventory" mnemonicParsing="false" prefHeight="75.0" prefWidth="150.0" text="Inventory" />
                           </children>
                           <VBox.margin>
                              <Insets right="25.0" />
                           </VBox.margin>
                           <padding>
                              <Insets right="50.0" />
                           </padding>
                        </VBox>
                        <VBox>
                           <children>
                          <Button fx:id="manageWorker" mnemonicParsing="false" prefHeight="75.0" prefWidth="150.0" text="Manage Workers">
                                 <VBox.margin>
                                    <Insets bottom="50.0" />
                                 </VBox.margin></Button>
                          <Button fx:id="manageStudent" mnemonicParsing="false" prefHeight="75.0" prefWidth="150.0" text="Manage Students" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
      </children>
    </StackPane>
  </children>
</VBox>
